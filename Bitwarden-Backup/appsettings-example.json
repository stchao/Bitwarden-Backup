{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Debug"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": null,
          "shared": true,
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "ExportFile": {
    "Directory": "", 
    "Name": "bw_export", 
    "DateInFileNameFormat": "yyyyMMdd", 
    "Format": "json", 
    "CustomExportPassword": "" 
  },
  "BitwardenConfiguration": {
    "Url": "https://vault.bitwarden.com", 
    "LogInMethod": "None",
    "EnableInteractiveLogIn": true, 
    "ExecutablePath": "" // 
  },
  "Credentials": {
    "EmailPasswordCredential": {
      "Email": "", 
      "MasterPassword": "", 
      "TwoFactorMethod": "None", 
      "TwoFactorCode": "" 
    },
    "APIKeyCredential": {
      "ClientId": "", 
      "ClientSecret": "",
      "MasterPassword": "" 
    }
  }
}
