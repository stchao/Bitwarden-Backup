{
  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": null,
          "shared": true,
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "isInteractive": true, // default is false
  "Credential": {
    "ClientId": "", // default is ""
    "ClientSecret": "", // default is ""
    "Url": "", // default is https://vault.bitwarden.com
    "Email": "", // default is ""
    "Password": "", // default is ""
    "TwoFactorCode": "" // default is ""
  }
}
