{
  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": null,
          "shared": true,
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "IsInteractive": true, // default is false
  "BitwardenExecutablePath": "", // default is "" (current executing directory)
  "ExportFile": {
    "Directory": "", // default is "" (current executing directory)
    "Name": "bw_export", // default is bw_export
    "DateInFileNameFormat": "yyyyMMdd", // default is "" (no date is appended to the file name)
    "Format": "", // default is json 
    "CustomExportPassword": "" // default is "" (file contents are encrypted with master password if format is json_encrypted)
  },
  "Credential": {
    "ClientId": "", // default is ""
    "ClientSecret": "", // default is ""
    "Url": "", // default is https://vault.bitwarden.com
    "Email": "", // default is ""
    "Password": "", // default is "",
    "TwoFactorMethod": "None", // default is "None"
    "TwoFactorCode": "" // default is ""
  }
}
