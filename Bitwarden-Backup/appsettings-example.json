{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Debug"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": null,
          "shared": true,
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "ExportFile": {
    "Directory": "", // default is "" (current executing directory)
    "Name": "bw_export", // default is bw_export
    "DateInFileNameFormat": "yyyyMMdd", // default is "" (no date is appended to the file name)
    "Format": "json", // default is json 
    "CustomExportPassword": "" // default is "" (file contents are encrypted with master password if format is json_encrypted)
  },
  "BitwardenConfiguration": {
    "Url": "", // default is https://vault.bitwarden.com
    "LogInMethod": "None", // default is "None"
    "EnableInteractiveLogIn": true, // default is true
    "ExecutablePath": "" // default is "" (current executing directory)
  },
  "Credentials": {
    "EmailPasswordCredential": {
      "Email": "", // default is ""
      "MasterPassword": "", // default is ""
      "TwoFactorMethod": "None", // default is "None"
      "TwoFactorCode": "" // default is ""
    },
    "APIKeyCredential": {
      "ClientId": "", // default is ""
      "ClientSecret": "", // default is ""
      "MasterPassword": "" // default is ""
    }
  }
}
